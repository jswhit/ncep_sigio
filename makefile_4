LIB	= ../../libsigio_4.a
INCMOD	= ../../incmod/sigio_4
FC	= $(FCS)
FFLAGS  = -traceback -g -xHOST -convert big_endian -assume byterecl  -I$(INCMOD) -FR
FFLAGB  = -O2 -traceback -g -xHOST -convert big_endian -assume byterecl 
ARFLAGS = -rv


$(LIB): $(LIB)(sigio_module.o) $(LIB)(sigio_r_module.o)  
$(LIB)(sigio_r_module.o):	$(LIB)(sigio_module.o)
$(LIB)(bafrio.o):	bafrio.f
	$(FC) -c $(FFLAGB) $<
	$(AR) $(ARFLAGS) $@ $*.o
	rm -f $*.o
.f.a:
	$(FC) -c $(FFLAGS) $<
	$(AR) $(ARFLAGS) $@ $*.o
	rm -f $*.o
	mv $*.mod $(INCMOD)
